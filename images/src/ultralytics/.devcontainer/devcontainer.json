{
    "name": "ultralytics Prebuilt Devcontainer Image",
    "build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
    "features": {
        "ghcr.io/devcontainers/features/common-utils": {},
        "ghcr.io/hellodword/devcontainer/common": {},
        "ghcr.io/devcontainers/features/python:1": {},
        "ghcr.io/hellodword/devcontainer/pip-dep": {
            "deps": "ultralytics onnx onnxruntime onnxconverter-common sahi tflite-runtime tensorflow"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": ["ms-python.autopep8"]
        }
    }
}
