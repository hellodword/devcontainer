{
    "name": "Unidbg development",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu-22.04",
    "features": {
        "ghcr.io/devcontainers/features/common-utils": {},
        "ghcr.io/hellodword/devcontainer/common": {},
        "ghcr.io/devcontainers/features/java:1": {
            "version": "17.0.10-jbr",
            "installMaven": "true",
            "mavenVersion": "3.8.6"
        },
        "ghcr.io/hellodword/devcontainer/maven-dep": {
            "git": "https://github.com/zhkl0228/unidbg"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": ["vscjava.vscode-java-pack", "redhat.vscode-xml"]
        }
    }
}
